{
    "version": "3.3",
    "services": {
        "webapp": {
            "image": "nginx",
            "ports": [
                "8080:80"
            ]
        },
        "webapp2": {
            "image": "httpd",
            "ports": [
                "8082:80"
            ]
        }
    }
}

# yaml
version: '3.3'
services:
        webapp:
                image: nginx
                ports:
                     - "8080:80"
        webapp2:
                image: httpd
                ports:
                     - "8082:80"
